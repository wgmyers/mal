;; Testing basic Ruby interop

;; Copied and modified from the lua/tests/stepA_mal.mal file

(ruby-eval "7")
;=>7

(ruby-eval "'7'")
;=>"7"

(ruby-eval "123 == 123")
;=>true

(ruby-eval "123 == 456")
;=>false

(ruby-eval "[7,8,9]")
;;=>(7 8 9)

(ruby-eval "{:abc => 789}")
;=>{"abc" 789}

(ruby-eval "puts \"hello\"")
;/hello
;=>nil

(ruby-eval "def foo; 8; end")
(ruby-eval "foo")
;=>8

(ruby-eval "'This sentence has five words'.gsub(/(\w+)/) { \"*#{$1.length}*\" }")
;;=>"*4* *8* *3* *4* *5*"

(ruby-eval "[3, 'a', 45, 'b'].join('|')")
;=>"3|a|45|b"
